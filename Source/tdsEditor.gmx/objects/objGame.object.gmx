uniSettings = shader_get_uniform(shdMain, "u_settings");
uniColour = shader_get_uniform(shdMain, "u_vignette_colour");
randomPosition = shader_get_uniform(shdMain, "random_pos");
mbQuality = shader_get_uniform(shdMain, "Quality");
uPoss = shader_get_uniform(shdMain, "poss");//uniform for x,y

ambientNoises[0] = ambGuitar1;
ambientNoises[1] = ambGuitar2;
ambientNoises[2] = ambRadio1;
ambientNoises[3] = ambRadio2;
ambientNoises[4] = ambGuitar3;
ambientNoises[5] = ambGuitar4;
ambientCount = array_length_1d(ambientNoises);

forcePlay = false;

minAmbient = room_speed * 85;
maxAmbient = room_speed * 360;
ambientTimer = irandom_range(minAmbient, maxAmbient);
lastAmbient = ambientNoises[irandom(ambientCount - 1)];

