        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Save Variables

lastBrowserWidth = browser_width;
lastBrowserHeight = browser_height;

//Disclaimer
if (keyboard_check_pressed(vk_f12))
    {
        var resourceCounts = gameGetResourceCounts();
        var spriteCount = string(resourceCounts[0]);
        var soundCount = string(resourceCounts[1]);
        var scriptCount = string(resourceCounts[2]);
        var objectCount = string(resourceCounts[3]);
        var roomCount = string(resourceCounts[4]);

        var resourceString = "Number of Resources: Sprites - " + spriteCount + ". Scripts - " + scriptCount + ". Objects - " + objectCount + ". Rooms - " + roomCount + ".";
        show_message_async(gameDemoDisclaimer + "#" + resourceString + "#" + gameVersion);
    }
    
    
//Ambient Noises (Guitar or otherwise)
if !ambientIsPlaying(ambientNoises) or (forcePlay)
    {
        if (ambientTimer &lt;= 0) or (forcePlay)
            {
                var checkAmbient = ambientNoises[irandom(ambientCount - 1)];
                
                while (checkAmbient == lastAmbient)
                    var checkAmbient = ambientNoises[irandom(ambientCount - 1)];
                    
                lastAmbient = checkAmbient;
                audio_play_sound(lastAmbient, 1, false);
                
                var _String = sound_get_name(lastAmbient);
                clAddMessage("Now playing ambient noise: " + _String, c_yellow);
                
                ambientTimer = irandom_range(minAmbient, maxAmbient);
                
                forcePlay = false;
            }
        else
            ambientTimer = max(ambientTimer - 1, 0);
    }
    

</string>
          </argument>
